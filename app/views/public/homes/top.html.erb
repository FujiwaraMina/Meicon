<div class="row justify-content-center my-3">
<!--メインビジュアル-->
  <%= image_tag "", size: "500x300" %>
</div>
<!--新着画像10件表示-->
<div class="row">
  <% @posts.last(10).each do |post| %>
    <%= link_to post_path(post) do %>
      <%= image_tag post.get_post_image(100, 100) %>
    <% end %>
  <% end %>
</div>